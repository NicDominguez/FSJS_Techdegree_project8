extends layout.pug

block content
    h1 Books
    p
        a.button(href="new_book.html") Create New Book
    table
        thead
            tr
                th Title
                th Author
                th Genre
                th Year
        tbody
            tr
                td
                    a(href="book_detail.html")= bookTitle
                td= bookAuthor
                td= bookGenre
                td= bookYear